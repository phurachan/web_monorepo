{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["server/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/agency-cms/(.*)",
      "dest": "/api/agency-cms/$1"
    },
    {
      "src": "/api/local-spot/(.*)",
      "dest": "/api/local-spot/$1"
    }
  ],
  "env": {
    "NUXT_ENV_VERCEL": "true"
  },
  "outputDirectory": ".output"
}